{
  "openapi": "3.0.1",
  "info": {
    "title": "Handshake",
    "version": "0.0.1"
  },
  "paths": {
    "/Post/getcloseposts": {
      "get": {
        "tags": [
          "Post"
        ],
        "summary": "Gets all posts nearby.",
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Post": {
      "post": {
        "tags": [
          "Post"
        ],
        "summary": "Posts a new post.",
        "requestBody": {
          "description": "The post to post.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertPostData"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertPostData"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InsertPostData"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/User/getcloseusers": {
      "get": {
        "tags": [
          "User"
        ],
        "summary": "Gets all users nearby.",
        "parameters": [
          {
            "name": "Longitude",
            "in": "query",
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "Latitude",
            "in": "query",
            "schema": {
              "type": "number",
              "format": "double"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/User": {
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Creates a new user.",
        "requestBody": {
          "description": "The user to create.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertUserDaten"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertUserDaten"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InsertUserDaten"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "InsertPostData": {
        "type": "object",
        "properties": {
          "author": {
            "type": "string",
            "description": "The author who made the post.",
            "format": "uuid"
          },
          "content": {
            "type": "string",
            "description": "The contetn of the post.",
            "nullable": true
          },
          "longitude": {
            "type": "number",
            "description": "The post location longitude;",
            "format": "double"
          },
          "latitude": {
            "type": "number",
            "description": "The post location latitude;",
            "format": "double"
          }
        },
        "additionalProperties": false,
        "description": "The insert post data is class for inserting new posts."
      },
      "InsertUserDaten": {
        "type": "object",
        "properties": {
          "nickname": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}